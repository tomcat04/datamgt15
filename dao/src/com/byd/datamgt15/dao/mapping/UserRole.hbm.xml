<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.byd.datamgt15.domain.UserRole" table="t_user_role">
        <id name="id" column="id" type="integer">
            <generator class="increment"/>
        </id>
        <property name="userId" column="USER_ID" type="string"  />
        <property name="roleId" column="ROLE_ID" type="integer"  />
        <property name="status" column="STATUS" type="string"  />
        <property name="requestTime" column="REQUEST_TIME" type="date"  />
    </class>
</hibernate-mapping>
