<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.byd.datamgt15.domain.MaterialExceptionLog" table="T_MATERIAL_EXCEPTION_LOG">
        <id name="id" column="id" type="integer">
            <generator class="increment"/>
        </id>
        <property name="materialCode" column="VENDOR_CODE" type="string"  /> 
        <property name="logTime" column="VENDOR_CODE" type="date"  /> 
        <property name="batch" column="VENDOR_CODE" type="string"  /> 
        <property name="exceptionDesc" column="VENDOR_CODE" type="string"  /> 
        <property name="dealDesc" column="VENDOR_CODE" type="string"  /> 
        <property name="status" column="VENDOR_CODE" type="string"  /> 
    </class>
</hibernate-mapping>
