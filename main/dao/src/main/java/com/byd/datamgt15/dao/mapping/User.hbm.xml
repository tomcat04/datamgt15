<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.byd.datamgt15.domain.User" table="t_user">
        <id name="id" column="id" type="integer">
            <generator class="increment"/>
        </id>
        <property name="region" column="REGION" type="string"  />
        <property name="userId" column="USER_ID" type="string"  />
        <property name="userName" column="USER_NAME" type="string"  />
        <property name="password" column="PASSWORD" type="string"  />
        <property name="email" column="EMAIL" type="string"  />
        <property name="gender" column="GENDER" type="string"  />
        <property name="jobTitle" column="JOB_TITLE" type="string"  />
        <property name="phone" column="PHONE" type="string"  />
        <property name="mobilePhone" column="MOBILE_PHONE" type="string"  />
        <property name="organization" column="ORGANIZATION" type="string"  />
        <property name="workingLocation" column="WORKING_LOCATION" type="string"  />
        <property name="workshop" column="WORKSHOP" type="string"  />
        <property name="workingSection" column="WORKING_SECTION" type="string"  />
        <property name="status" column="STATUS" type="string"  />
    </class>
</hibernate-mapping>
